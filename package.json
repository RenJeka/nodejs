{
  "name": "2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "tsc --project \"backend.tsconfig.json\" -w",
    "dev": "npx nodemon",
    "clear": "rimraf ./build",
    "copy": "copyfiles -u 1  -e \"src/**/*.ts\" \"src/**/*\" \"build/\"",
    "ts-backend": "npx tsc --project \"backend.tsconfig.json\"",
    "ts-frontend": "npx tsc --project \"frontend.tsconfig.json\"",
    "build": "npm run clear && npm run ts-backend && npm run ts-frontend && npm run copy",
    "start": "npm run build && node build/app.js",
    "start-js": "nodemon ./build/app.js",
    "dev-js": "nodemon --inspect ./build/app.js",
    "test": "jest",
    "test2": "node --experimental-vm-modules node_modules/jest/bin/jest.js "
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "colors": "1.4.0",
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "16.0.3",
    "ejs": "2.6.1",
    "express": "4.16.1",
    "express-mysql-session": "3.0.0",
    "express-session": "1.17.3",
    "http-errors": "1.6.3",
    "morgan": "1.9.1",
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "18.15.11",
    "@types/supertest": "2.0.12",
    "copyfiles": "2.4.1",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "rimraf": "5.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
