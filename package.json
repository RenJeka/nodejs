{
  "name": "2",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "watch": "tsc -w",
    "dev": "npx nodemon",
    "clear": "rimraf ./build",
    "copy": "copyfiles -u 1 -e \"./**/*.js\" -e \"./**/*.ts\" \"src/**/*\" \"build/\"",
    "build": "npm run clear && npx tsc && npm run copy",
    "start": "npm run build && node build/bin/www.js",
    "start-js": "nodemon ./build/bin/www.js",
    "dev-js": "nodemon --inspect ./build/bin/www.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "colors": "1.4.0",
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "16.0.3",
    "ejs": "2.6.1",
    "express": "4.16.1",
    "express-mysql-session": "3.0.0",
    "express-session": "1.17.3",
    "http-errors": "1.6.3",
    "morgan": "1.9.1",
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "18.15.11",
    "copyfiles": "2.4.1",
    "nodemon": "2.0.22",
    "rimraf": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
